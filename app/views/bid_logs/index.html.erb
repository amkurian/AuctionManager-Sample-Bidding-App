<p id="notice"><%= notice %></p>

<h1>Listing Bid Logs</h1>

<table>
  <thead>
    <tr>
      <th>Amount</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bid_logs.each do |bid_log| %>
      <tr>
        <td><%= bid_log.amount %></td>
        <td><%= bid_log.user %></td>
        <td><%= link_to 'Show', bid_log %></td>
        <td><%= link_to 'Edit', edit_bid_log_path(bid_log) %></td>
        <td><%= link_to 'Destroy', bid_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bid log', new_bid_log_path %>
